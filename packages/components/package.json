  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Developer-DAO/web3-ui/"
  },
  "scripts": {
    "build": "preconstruct build",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
