    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "cross-fetch": "^3.1.4",
    "ethers": "^5.5.2",
    "framer-motion": "^4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@storybook/react": "^6.3.12",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^16.9.10",
    "@web3-ui/hooks": "^0.4.1",
    "babel-loader": "^8.2.1",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.0",
    "webpack": "^4.42.1"
