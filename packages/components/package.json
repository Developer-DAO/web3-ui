{
  "name": "@web3-ui/components",
  "license": "MIT",
  "version": "0.2.2",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "yarn": "^1.5"
  },
  "description": "React UI components for web3",
  "author": "Developer DAO",
  "homepage": "https://github.com/Developer-DAO/web3-ui/",
